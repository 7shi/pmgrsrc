#!/bin/sh
# This file is in the public domain.

if [ $# -gt 1 ]
then
	type=$1
	shift
	target=$1
	shift

	case $type in
		z)
			pmgr-sizecat $target | gzip -dc | tar xf - "$@"
			exit
			;;
		j)
			pmgr-sizecat $target | pmgr-bzip2 -dc | tar xf - "$@"
			exit
			;;
	esac
fi

echo "usage: `basename $0` z/j archive"
exit 1
